source `dirname $BASH_ARGV`/git_prompt

alias gs='git status'
alias gl='git pull'
alias gp='git push'
alias gd='git diff | mate'
alias ga='git add'
alias gc='git commit -v'
alias gca='git commit -v -a'
alias gb='git branch'
alias gba='git branch -a'
alias gco='git checkout'
alias gdc='git-svn dcommit'
alias gk='gitk -all &'

