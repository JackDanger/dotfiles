alias gemi='sudo gem install --no-rdoc --no-ri'
alias e='$EDITOR'
alias trail='tail -n 300 -f'
alias console='ruby script/server'
alias ss='mongrel_rails start'
alias rs='mongrel_rails restart'
alias sts='mongrel_rails stop'
alias ssd='mongrel_rails start -d'
alias sslog='mongrel_rails start -d | tail -f log/development.log'
alias psm='ps aux | grep mongrel'
alias rollback='cap rollback'
alias deploy='cap deploy'
alias ignore='svn pe svn:ignore'
alias addnew='for i in `st | grep '^\?'`; do add $i; done;'
alias routes='rake routes | less'
alias clone='rake db:test:clone_structure'
alias migrate='rake db:migrate'
alias remigrate='rake db:migrate VERSION=0 && rake db:migrate'
alias remigrate_prod='rake db:migrate VERSION=0 RAILS_ENV=production && rake db:migrate RAILS_ENV=production'
alias remigrate_dev='rake db:migrate VERSION=0 RAILS_ENV=development && rake db:migrate RAILS_ENV=development'
alias sql='mysql'
alias asql='mysql adpickles_development'
alias dump='mysqldump -u root -p --compact --extended-insert=false --default-character-set=latin1'
alias tcap='rake && cap'
alias grep='grep --color=auto'
alias l='ls -la'
alias st='svn st'
alias sti='svn st --ignore-externals | grep -v ^X'
alias u='echo "currently at: `current_revision`" && svn --ignore-externals update'
alias up='svn update'
alias update='svn update'
alias delete='svn delete'
alias add='svn add'
alias copy='svn copy'
alias move='svn move'
alias commit='svn commit'
alias sf='svn diff'
alias slog='svn log | head'
alias uslog='svn update && svn log | head'
alias sinfo='svn info'
alias ksvn='killall -9 svn'
alias resolved='svn resolved'
