#/bin/bash
if [[ -n $GOPATH ]]; then
  add_to_path $GOPATH/bin
  # Edit a go package
  function gov {
    vim `golang-package $1`
  }

  function gocd {
    cd `golang-package $1`
  }

  function golang-package {
    find $GOPATH/src/(square|*.(com|org|net))/* -type d -maxdepth 1 | grep $1 | head -n 1
  }
fi
# Test helpers moved to ./bin/
